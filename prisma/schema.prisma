// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["dataProxy"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Log {
  id      String @id
  level   Level
  message String
  meta    Json
}

enum Level {
  Info
  Warn
  Error
}